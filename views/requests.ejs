<span class="W100 left bl">
  <span class="W50 left bl">
    
    <span class="left bl W100 b_purple white bord_white P2">They Requested</span>

    <% for(var i=0; i<to_me.length; i++) { %>

      <span id="<%= to_me[i].id %>" class="b_purple white square left bl bord_white P2">
        From: <%= to_me[i].asker.name %>
        Re: <%= to_me[i].thought.body %>
        <button class="accept" id="<%= to_me[i].id %>">Accept</button>
      </span>

    <% } %>
  </span>

  <span class="W50 left bl">
    
    <span class="left bl W100 b_purple white bord_white P2">I Requested</span>

    <% for(var i=0; i<from_me.length; i++) { %>

      <span class="b_purple white square left bl bord_white P2">
        From: Me
        Re: <%= from_me[i].body %>
      </span>

    <% } %>
  </span>
</span>